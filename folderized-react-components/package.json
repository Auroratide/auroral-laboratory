{
  "name": "folderized-react-components",
  "version": "1.0.0",
  "description": "Demonstrates how to group all a component's files into a single folder",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && serve public",
    "build": "npm-run-all build:html build:webpack",
    "build:html": "mkdir -p public && cp src/index.html public/index.html",
    "build:webpack": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Timothy Foster",
  "license": "ISC",
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "serve": "^10.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "mini-css-extract-plugin": "^0.4.3",
    "npm-run-all": "^4.1.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  }
}
